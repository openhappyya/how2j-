public class Hero{
	String name; //姓名
    float hp; //血量
    float armor; //护甲
    int moveSpeed; //移动速度
    

    void legendary() {
    	//超神
    	System.out.println("超神")
    }
    
    float getHp() {
    	//获取当前血量
    	return hp;
    }
    
    float recovery(float blood) {
    	//回血
    	blood=blood+hp;
    }

}
